<section class="content">
<div class="row">
  <div class="col-md-12">

    <div class="box">
      <div class="box-header">
        <h3 class="box-title">{{model.id}}</h3>
      </div>
      <br>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <div class="box">
      <div class="box-header">
        <div class="row">
          <div class="col-xs-6">
            {{input value=queryText type="text" class="form-control" placeholder="Search"}}
          </div>
          <div class="col-xs-6">
            <ul class="pagination pagination-sm no-margin pull-right">
              {{#if hasPrev}}
                <li><a style="cursor: pointer;" {{action 'prevPage'}}>«</a></li>
              {{/if}}
              <li><a>{{page}}</a></li>
              {{#if hasNext}}
                <li><a style="cursor: pointer;" {{action 'nextPage'}}>»</a></li>
              {{/if}}
            </ul>
          </div>
        </div>
      </div>
      <div class="box-body">
        {{#if history.isPending}}
          <div style="text-align: center;">
            <i class="fa fa-spinner fa-pulse"></i>
          </div>
        {{else}}
          {{#history-tree data=tableData}}
            <colgroup>
              <col width="40%">
              <col width="50%">
              <col width="10%">
            </colgroup>
            <thead>
              <tr>
                <th>Key</th>
                <th>Value</th>
                <th>Type</th>
              </tr>
            </thead>
            <tbody>
            </tbody>
          {{/history-tree}}
        {{/if}}
      </div>
    </div>
  </div>
</div>
</section>

{{outlet}}
